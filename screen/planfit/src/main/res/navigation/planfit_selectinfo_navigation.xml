<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/planfit_selectinfo_navigation"
    app:startDestination="@id/selectInfoExerciseLevelFragment">


    <fragment
        android:id="@+id/selectInfoExerciseLevelFragment"
        android:name="com.potatomeme.screen.planfit.presentation.on_board.SelectInfoExerciseLevelFragment"
        android:label="SelectInfoExerciseLevelFragment">
        <action
            android:id="@+id/action_selectInfoExerciseLevelFragment_to_selectInfoPlaceFragment"
            app:destination="@id/selectInfoPlaceFragment"
            app:enterAnim="@anim/side_in_right"
            app:exitAnim="@anim/keep_state"
            app:popEnterAnim="@anim/side_in_left"
            app:popExitAnim="@anim/keep_state" />
    </fragment>
    <fragment
        android:id="@+id/selectInfoPlaceFragment"
        android:name="com.potatomeme.screen.planfit.presentation.on_board.SelectInfoPlaceFragment"
        android:label="SelectInfoPlaceFragment" >
        <action
            android:id="@+id/action_selectInfoPlaceFragment_to_selectInfoEquipmentTypeFragment"
            app:destination="@id/selectInfoEquipmentTypeFragment"
            app:enterAnim="@anim/side_in_right"
            app:exitAnim="@anim/keep_state"
            app:popEnterAnim="@anim/side_in_left"
            app:popExitAnim="@anim/keep_state"  />
        <action
            android:id="@+id/action_selectInfoPlaceFragment_to_selectInfoGymFragment"
            app:destination="@id/selectInfoGymFragment" />
    </fragment>
    <fragment
        android:id="@+id/selectInfoEquipmentTypeFragment"
        android:name="com.potatomeme.screen.planfit.presentation.on_board.SelectInfoEquipmentTypeFragment"
        android:label="SelectInfoEquipmentTypeFragment" />
    <fragment
        android:id="@+id/selectInfoGymFragment"
        android:name="com.potatomeme.screen.planfit.presentation.on_board.SelectInfoGymFragment"
        android:label="SelectInfoGymFragment" />
</navigation>